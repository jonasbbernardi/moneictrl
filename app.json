{
    "expo": {
        "name": "Monei Ctrl",
        "slug": "moneictrl",
        "version": "1.0.0",
        "orientation": "portrait",
        "icon": "./assets/icon.png",
        "splash": {
            "image": "./assets/splash.png",
            "resizeMode": "contain",
            "backgroundColor": "#50509F"
        },
        "updates": {
            "fallbackToCacheTimeout": 0
        },
        "assetBundlePatterns": [
            "**/*"
        ],
        "ios": {
            "bundleIdentifier": "br.com.jonasbbernardi.moneictrl",
            "buildNumber": "1.0.0",
            "supportsTablet": true
        },
        "android": {
            "package": "br.com.jonasbbernardi.moneictrl",
            "versionCode": 1,
            "adaptiveIcon": {
                "foregroundImage": "./assets/adaptive-icon.png",
                "backgroundColor": "#50509F"
            }
        },
        "web": {
            "favicon": "./assets/favicon.png"
        }
    }
}
